{
  "name": "@blockless/cli",
  "version": "0.0.1-development",
  "description": "blockless cli client, manage, interact with and deploy blockless applications.",
  "main": "src/index.ts",
  "private": false,
  "bin": {
    "bls": "dev-bin.js"
  },
  "scripts": {
    "build:module": "tsc",
    "build": "tsc &&  pkg -t node14-linux-x64,node14-windows-x64,node14-macos-x64 ./bin.js -o bls_x64",
    "build:arm": "tsc &&  pkg -t node14-linux-arm64,node14-windows-arm64,node14-macos-arm64 ./bin.js -o bls_arm64",
    "format": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "files": [
    "dev-bin.js",
    "bin.js",
    "dist/**/*.js",
    "dist/*.json",
    "src/*"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@cosmjs/launchpad": "^0.27.1",
    "@fastify/cors": "^8.3.0",
    "@fastify/rate-limit": "^7.6.0",
    "@iarna/toml": "^2.2.5",
    "@types/node": "^18.0.3",
    "axios": "^0.27.2",
    "chalk": "^2.4.2",
    "cli-table3": "^0.6.3",
    "compare-versions": "^5.0.1",
    "dayjs": "^1.11.9",
    "fastify": "^4.8.1",
    "follow-redirects": "^1.15.1",
    "form-data": "^4.0.0",
    "js-yaml": "^4.1.0",
    "lowdb": "^1.0.0",
    "open": "^8.4.0",
    "pkg": "^5.8.0",
    "portastic": "^1.0.1",
    "portfinder": "^1.0.32",
    "prompt": "^1.3.0",
    "prompts": "^2.4.2",
    "readable-stream": "^4.1.0",
    "replace-in-file": "^6.3.5",
    "simple-git": "^3.14.1",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4",
    "unique-names-generator": "^4.7.1",
    "yargs": "^17.6.2",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@types/follow-redirects": "^1.14.1",
    "@types/iarna__toml": "^2.0.2",
    "@types/js-yaml": "^4.0.5",
    "@types/prompt": "^1.1.2",
    "@types/prompts": "^2.0.14",
    "@types/terminal-kit": "^1.34.1",
    "@types/yargs": "^17.0.15",
    "prettier": "^3.0.1"
  }
}
